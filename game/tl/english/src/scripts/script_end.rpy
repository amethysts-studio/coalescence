    # narr "Le Bourreau hésita un instant fatal."
    narr "The Executioner hesitate, for one deadly moment..."

# game/script_end.rpy:917
translate english ending_chemist_no_sacrifice_5e941a80_1:

    # narr "Erwin se leva."
    narr "Erwin got up."

# game/script_end.rpy:918
translate english ending_chemist_no_sacrifice_b77e83ed:

    # narr "Il prit le bras du Bourreau."
    narr "He took the Executioner by the arm."

