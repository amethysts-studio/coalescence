    # narr "Il mit le pied sur la tête du Bourreau, pour lui enlever sa capuche."
    narr "He removed the Executioner's hood with his foot."

# game/script_end.rpy:910
translate english ending_chemist_no_sacrifice_d0cecb11:

    # j "Aussi connu sous le nom de..."
    j "Also known as..."

# game/script_end.rpy:912
translate english ending_chemist_no_sacrifice_daba055e:

    # n ""
    n ""

